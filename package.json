    "sass": "^1.49.9"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "serverless": "^2.72.2",
    "tailwind": "^4.0.0",
    "typescript": "^4.6.2"
  }
